{
  "manifest_version": 2,

  "name": "Verified",
  "description": "Real Twitter account verificator",
  "version": "1.0",

  "permissions": [
    "activeTab",
    "https://twitter.com/"
  ],
  "content_scripts": [
    {
      "matches": ["*://twitter.com/*"],
      "js": ["jquery.js", "script.js"]
    }
  ]
}
